<%- include('partials/header'); -%>

<main>
    <h1>The Good Surf</h1>
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img class="d-block w-100" src="../images/surf.jpg" alt="First slide">
            <p>The Best Place for Latest New On Surf</p>
          </div>
          <div class="carousel-item">
            <img class="d-block w-100" src="../images/windsurf.jpg" alt="Second slide">
          </div>
          <div class="carousel-item">
            <img class="d-block w-100" src="../images/windsurf2.jpg" alt="Third slide">
          </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>

      <section>
        <h2>Latest News</h2>
      </section>

      <section>
        <h2>Spot Checks</h2>
        <p>Info and Reviews on Surf Spots around the world </p>
      </section>

      <section>
        <h2>Product Reviews</h2>
        <p>Reviews on latest Surf Equipment, Swimwear and Asseccories</p>
      </section>




</main>


<%- include('partials/footer'); -%>
